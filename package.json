{
  "name": "somm-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest --watch",
    "coverage": "jest --coverage"
  },
  "dependencies": {
    "next": "10.1.3",
    "react": "^17.0.2",
    "react-dom": "17.0.2",
    "styled-components": "^5.2.3",
    "swr": "^0.5.5",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^13.1.5",
    "babel-jest": "^26.6.3",
    "babel-plugin-styled-components": "^1.12.0",
    "jest": "^26.6.3",
    "msw": "^0.28.2"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.js"
    ],
    "moduleNameMapper": {
      "^components/(.*)$": "<rootDir>/components/$1",
      "^lib/(.*)$":  "<rootDir>/lib/$1",
      "^hooks/(.*)$":  "<rootDir>/hooks/$1",
      "^mocks/(.*)$":  "<rootDir>/mocks/$1",
      "^pages/(.*)$":  "<rootDir>/pages/$1"
    }
  }
}
